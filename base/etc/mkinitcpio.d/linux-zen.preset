# mkinitcpio preset file for the 'linux-zen' package

ALL_config="/etc/mkinitcpio.d/default.conf"
ALL_kver="/boot/kernel/vmlinuz-linux-zen"
ALL_kerneldest="/boot/kernel/vmlinuz-linux-zen"

PRESETS=('default')
